---
controller_templates_all:
{%- for job_template in job_templates -%}
{{ '\n' -}} - name: {{ job_template.name }}
 {%- if job_template.description != '' -%} 
{{ '\n' -}}{{ '\t' -}}description: {{ job_template.description }}
 {%- endif -%}
{{ '\n' -}}{{ '\t' -}}project: {{ job_template.project.name }}
  job_type: {{ job_template.job_type }}
  playbook: {{ job_template.playbook }}
  inventory: {{ job_template.inventory.name }}
{%- if job_template.execution_environment is not none -%}
{{ '\n' -}}{{ '\t' -}}execution_environment: {{ job_template.execution_environment.name }}
{%- endif -%}
{{ '\n' -}}{{ '\t' -}}verbosity: {{ job_template.verbosity }}
  credentials: {{ '\n' -}}
     {%- for credential in job_template.related.credentials -%}
     {{ '\t' -}}{{ '\t' -}}- {{ credential.name }} {{ '\n' -}}
     {% endfor -%}
{{ '\t' -}}extra_vars: {{ job_template.extra_vars }}
{%- if job_template.job_tags != '' -%} 
{{ '\n' -}}{{ '\t' -}}job_tags: {{ job_template.job_tags }}
 {%- endif -%}
{%- if job_template.scm_branch != '' -%} 
{{ '\n' -}}{{ '\t' -}}scm_branch: {{ job_template.scm_branch }}
 {%- endif -%}
{%- if job_template.skip_tags != '' -%} 
{{ '\n' -}}{{ '\t' -}}skip_tags: {{ job_template.skip_tags }}
 {%- endif -%}
{%- if job_template.start_at_task != '' -%} 
{{ '\n' -}}{{ '\t' -}}start_at_task: {{ job_template.start_at_task }}
 {%- endif -%}
{%- if job_template.host_config_key != '' -%} 
{{ '\n' -}}{{ '\t' -}}host_config_key: {{ job_template.host_config_key }}
 {%- endif -%}
 {%- if job_template.webhook_service != '' -%} 
{{ '\n' -}}{{ '\t' -}}webhook_service: {{ job_template.webhook_service }}
 {%- endif -%}
 {%- if job_template.webhook_credential is not none -%} 
{{ '\n' -}}{{ '\t' -}}webhook_credential: {{ job_template.webhook_credential }}
 {%- endif -%}
 {%- if job_template.ask_scm_branch_on_launch -%} 
{{ '\n' -}}{{ '\t' -}}ask_scm_branch_on_launch: {{ job_template.ask_scm_branch_on_launch }}
 {%- endif -%}
{%- if job_template.ask_diff_mode_on_launch -%} 
{{ '\n' -}}{{ '\t' -}}ask_diff_mode_on_launch: {{ job_template.ask_diff_mode_on_launch }}
 {%- endif -%}
{%- if job_template.ask_variables_on_launch -%} 
{{ '\n' -}}{{ '\t' -}}ask_variables_on_launch: {{ job_template.ask_variables_on_launch }}
 {%- endif -%}
{%- endfor -%}